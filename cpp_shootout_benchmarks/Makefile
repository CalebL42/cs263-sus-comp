all: binary-trees fannkuch-redux fasta spectral-norm

binary-trees: binary-trees.cpp
	g++ binary-trees.cpp -o binary-trees.o -lCPP_Joules

fannkuch-redux: fannkuch-redux.cpp
	g++ fannkuch-redux.cpp -o fannkuch-redux.o -lCPP_Joules

fasta: fasta.cpp
	g++ fasta.cpp -o fasta.o -lCPP_Joules

spectral-norm: spectral-norm.cpp
	g++ spectral-norm.cpp -o spectral-norm.o -lCPP_Joules

clean:
	rm -f *.o
